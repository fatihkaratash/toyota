[
  {
    "name": "RESTProvider1",
    "type": "REST",
    "enabled": true,
    "implementationClass": "com.toyota.mainapp.subscriber.impl.RestRateSubscriber",
    "connectionConfig": {
      "baseUrl": "http://localhost:8081",
      "pollIntervalMs": 5000,
      "symbols": ["USD/TRY", "EUR/TRY", "GBP/TRY"],
      "timeout": 3000,
      "retryConfig": {
        "maxAttempts": 3,
        "waitDuration": 1000
      },
      "circuitBreakerConfig": {
        "failureThreshold": 50,
        "waitDurationMs": 30000
      }
    }
  },
  {
    "name": "TCPProvider2",
    "type": "TCP",
    "enabled": true,
    "implementationClass": "com.toyota.mainapp.subscriber.impl.TcpRateSubscriber",
    "connectionConfig": {
      "host": "localhost",
      "port": 8080,
      "connectionTimeoutMs": 5000,
      "retryAttempts": 3,
      "symbols": ["USD/TRY", "EUR/TRY"]
    }
  }
]